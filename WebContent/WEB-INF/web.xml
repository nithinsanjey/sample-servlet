<web-app>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>session.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/LoginProcess</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>session.Logout</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/LogoutProcess</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Profile</servlet-name>
		<servlet-class>session.Profile</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Profile</servlet-name>
		<url-pattern>/ProfileProcess</url-pattern>
	</servlet-mapping>
	
	<context-param>
		<param-name>MysqlUrl</param-name>
		<param-value>jdbc:mysql://localhost:3306/mydb?autoReconnect=true&amp;useSSL=false</param-value>
	</context-param>
	<context-param>
		<param-name>MysqlDriver</param-name>
		<param-value>com.mysql.jdbc.Driver</param-value>
	</context-param>
	
</web-app>